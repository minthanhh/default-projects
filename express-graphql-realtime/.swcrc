{
    "$schema": "https://json.schemastore.org/swcrc",
    "module": {
        "type": "commonjs",
        "resolveFully": true
    },
    "jsc": {
        "minify": {
            "compress": {
                "arguments": false,
                "arrows": true,
                "booleans": true,
                "booleans_as_integers": false,
                "collapse_vars": true,
                "comparisons": true,
                "computed_props": true,
                "conditionals": true,
                "dead_code": true,
                "directives": true,
                "drop_console": false,
                "drop_debugger": true,
                "evaluate": true,
                "expression": false,
                "hoist_funs": false,
                "hoist_props": true,
                "hoist_vars": false,
                "if_return": true,
                "join_vars": true,
                "keep_classnames": false,
                "keep_fargs": true,
                "keep_fnames": false,
                "keep_infinity": false,
                "loops": true,
                "negate_iife": true,
                "properties": true,
                "reduce_funcs": false,
                "reduce_vars": false,
                "side_effects": true,
                "switches": true,
                "typeofs": true,
                "unsafe": false,
                "unsafe_arrows": false,
                "unsafe_comps": false,
                "unsafe_Function": false,
                "unsafe_math": false,
                "unsafe_symbols": false,
                "unsafe_methods": false,
                "unsafe_proto": false,
                "unsafe_regexp": false,
                "unsafe_undefined": false,
                "unused": true,
                "const_to_let": true,
                "pristine_globals": true
            },
            "mangle": {
                "toplevel": false,
                "keep_classnames": false,
                "keep_fnames": false,
                "keep_private_props": false,
                "ie8": false,
                "safari10": false
            }
        },
        "parser": {
            "syntax": "typescript",
            "dynamicImport": true,
            "decorators": true,
            "importAssertions": true,
            "noEarlyErrors": true,
            "dts": true,
            "tsx": false
        },
        "transform": {
            "decoratorMetadata": true,
            "legacyDecorator": true,
            "useDefineForClassFields": true
        },
        "target": "es2016",
        "baseUrl": "src",
        "preserveAllComments": true,
        "keepClassNames": true,
        "loose": false,
        "externalHelpers": true,
        "experimental": {
            "keepImportAssertions": true
        }
        // Requires v1.2.50 or upper and requires target to be es2016 or upper.
        //   "keepClassNames": false
    },
    "isModule": true,
    "sourceMaps": true,
    "minify": true
    // "swcrc": true,
    // "inputSourceMap": true
}
